pipeline {
  
  agent any
  
  stages {
    
    stage("build") {
      steps {
        echo 'building the appliacion...'     
      }
    }
    
    stage("test") {  
      steps {
        echo 'testing the appliacion...'
      }
    }
    
    stage("deploy") {
      steps {
        echo 'deploying the appliacion...'
      }
    }
    
  }
}
